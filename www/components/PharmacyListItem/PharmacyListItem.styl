@require('../../assets/styles/vars.styl')
@require('../../assets/styles/font-awesome.css')

.PharmacyListItem
  &__Container
    color $green
    padding 20px
    text-align left
    width 100%
    height 100%
    overflow-y hidden
    overflow-x hidden
    box-sizing border-box
  &__Links
    padding-top 6px
    div
      display inline-block
      a
        color $off-white
        text-decoration none
  &__Name
    font-size 16px
  &__Address
    font-size 13px